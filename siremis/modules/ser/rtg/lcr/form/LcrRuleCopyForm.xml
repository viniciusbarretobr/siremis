<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="LcrRuleCopyForm" Class="EasyForm" FormType="Edit" jsClass="jbForm" Title="Copy Lcr Rule" Description="" BizDataObj="ser.rtg.lcr.do.LcrRuleDO" TemplateEngine="Smarty" TemplateFile="detail.tpl" EventName="" MessageFile="">
