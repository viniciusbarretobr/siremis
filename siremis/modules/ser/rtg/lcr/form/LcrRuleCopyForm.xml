        <Element Name="btn_save" Class="Button" Text="Save" CssClass="button_gray_m">
            <EventHandler Name="save_onclick" EventLogMsg="" Event="onclick" Function="InsertRecord()" RedirectPage="form=ser.rtg.lcr.form.LcrRuleDetailForm&amp;fld:Id={@ser.rtg.lcr.do.LcrRuleDO:Field[Id].Value}"  ShortcutKey="Ctrl+Enter" ContextMenu="Save" />
        </Element>
        <Element Name="btn_cancel" Class="Button" Text="Cancel" CssClass="button_gray_m">
            <EventHandler Name="cancel_onclick" Event="onclick" Function="SwitchForm(ser.rtg.lcr.form.LcrRuleListForm)"  ShortcutKey="Escape" ContextMenu="Cancel"/>
