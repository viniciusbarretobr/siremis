<?xml version="1.0" encoding="UTF-8"?>
<EasyForm Name="PresentityCopyForm" Class="EasyForm" FormType="Edit" jsClass="jbForm" Title="Copy Presentity" Description="" BizDataObj="sipadmin.prs.presence.do.PresentityDO" TemplateEngine="Smarty" TemplateFile="detail.tpl" EventName="" MessageFile="">
